{
  "name": "Would You Like to Support",
  "description": "Support your favorite content creators by using their afilliate code(s)!",
  "author": "Jonathan Goss",
  "version": "1.0",
  "manifest_version": 2,

  "permissions": ["tabs"],

  "browser_action": {
    "default_popup": "dashboard.html",
    "default_icon": "icons/letter-w-32px.png"
  },

  "icons": {
    "16"  :  "icons/letter-w-16px.png",
    "32"  :  "icons/letter-w-32px.png",
    "48"  :  "icons/letter-w-48px.png",
    "128" :  "icons/letter-w-128px.png"
  }
}
